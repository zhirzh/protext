{
  "name": "protext",
  "version": "1.0.0",
  "author": "Shirsh Zibbu <zibbu.shirsh+github@gmail.com>",
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "dependencies": {
    "opentype.js": "^0.7.2",
    "replacestream": "^4.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-babel-webpack-plugin": "^1.1.0",
    "webpack": "^3.2.0"
  },
  "babel": {
    "plugins": [
      "babel-plugin-transform-class-properties"
    ],
    "presets": [
      "env",
      "flow"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ]
    }
  },
  "scripts": {
    "start": "webpack -w",
    "build": "webpack -p"
  }
}
